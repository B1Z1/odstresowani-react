    "test": {
      "root": "apps/test",
      "sourceRoot": "apps/test",
      "projectType": "application",
      "targets": {
        "build": {
          "executor": "@nrwl/next:build",
          "outputs": ["{options.outputPath}"],
          "options": { "root": "apps/test", "outputPath": "dist/apps/test" },
          "configurations": { "production": {} }
        },
        "serve": {
          "executor": "@nrwl/next:server",
          "options": { "buildTarget": "test:build", "dev": true },
          "configurations": {
            "production": {
              "buildTarget": "test:build:production",
              "dev": false
            }
          }
        },
        "export": {
          "executor": "@nrwl/next:export",
          "options": { "buildTarget": "test:build:production" }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/apps/test"],
          "options": {
            "jestConfig": "apps/test/jest.config.js",
            "passWithNoTests": true
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": { "lintFilePatterns": ["apps/test/**/*.{ts,tsx,js,jsx}"] }
        }
      }
