{
  "kind": "collectionType",
  "collectionName": "categories",
  "info": {
    "name": "Category",
    "description": ""
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": true
  },
  "attributes": {
    "name": {
      "type": "string",
      "private": false,
      "required": true,
      "maxLength": 64
    },
    "categories": {
      "collection": "category"
    },
    "seo": {
      "type": "component",
      "repeatable": false,
      "component": "seo.seo",
      "required": true
    },
    "slug": {
      "type": "string",
      "required": false,
      "unique": true
    },
    "posts": {
      "via": "categories",
      "collection": "post"
    }
  }
}
